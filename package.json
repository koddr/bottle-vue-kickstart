    "postcss-loader": "^2.1.6",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.18.2",
    "vue-loader": "^13.7.3",
    "vue-style-loader": "^3.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^3.12.0"

    "css-loader": "^5.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "node-sass": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.3.1",
    "vue-loader": "^15.9.6",
    "style-loader": "^0.23.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^3.12.0"
  },
